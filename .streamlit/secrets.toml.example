# Zuper API Configuration
[zuper]
api_key = "your_zuper_api_key_here"
base_url = "https://us-east-1.zuperpro.com/api"

# Anthropic API Configuration (for AI-powered features)
[anthropic]
api_key = "your_anthropic_api_key_here"

# Application Settings (optional)
[app]
refresh_interval_minutes = 15
max_jobs_per_page = 50

# NOTE: No database configuration needed!
# This app uses SQLite which stores data locally in data/eu_parts_jobs.db
